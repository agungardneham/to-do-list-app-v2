<%- include("header") -%>

<div class="box" id="heading">
    <h1> <%= otherListTitle %> </h1>
  </div>

  <div class="box">
    <% otherNewListItems.forEach(function(a){ %>
      <form action="/delete" method="post">
        <div class="item">
          <input type="checkbox" onChange="this.form.submit()" name="checkbox" value="<%= a._id %>">
          <p><%=  a.name  %></p>
        </div>
      </form>
      <% }) %>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list">+</button>
      </form>
  </div>

<%- include("footer") -%>